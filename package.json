{
  "name": "gardenz",
  "version": "0.1.1",
  "main": "server/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harakeke-2021/gardenz.git"
  },
  "scripts": {
    "start": "node server",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode=production",
    "build:server": "npm run db:migrate",
    "webpack": "webpack --config client/webpack.config.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate && npm run db:seed",
    "test": "jest --testPathIgnorePatterns e2e --verbose",
    "test:watch": "jest --testPathIgnorePatterns e2e --watchAll --verbose",
    "test:e2e": "NODE_ENV=e2e PORT=3001 server-test 3001 e2e",
    "test:e2e:debug": "DEBUG=pw:api npm run test:e2e",
    "e2e": "jest e2e --config e2e/jest.config.js --verbose",
    "lint": "eslint --ext .js,.jsx .",
    "docker": "npm run db:migrate && npm run db:seed && npm run start"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eda",
      "plugin:jest-playwright/recommended"
    ],
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "engines": {
    "node": ">=12.0"
  },
  "dependencies": {
    "authenticare": "^0.4.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.12",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "bulma": "^0.9.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.4.5",
    "leaflet": "^1.7.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.0.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "sqlite3": "^5.0.0",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^2.0.0",
    "superagent": "^6.1.0",
    "supertest": "^6.0.1",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
